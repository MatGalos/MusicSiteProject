@model ICollection<MusicSite.Models.User>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>Liczba Użytkowników: @Model.Count</h1>
<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>User Name</th>
        <th>Password</th>
        <th>E-Mail address</th>
        <th>Date of Birth</th>
        <th>Number of Reviews</th>
    </tr>
    <tbody>
        @foreach(var u in @Model)
            {
        <tr>
            <td>
                @u.name
            </td>
            <td>
                @u.lastName
            </td>
            <td>
                @u.userName
            </td>
            <td>
                @u.password
            </td>
            <td>
                @u.eMail
            </td>
            <td>
                @u.creationDate
            </td>
            <td>
                @u.numberOfReviews
            </td>
        </tr>
            }
    </tbody>
</table>